<style>
  .ship-preview-container {
  box-shadow: #d4d4d4 -1px 0 0,rgba(0,0,0,0.05) 1px 0 2px inset;
  background-image: url('http://hull.io/images/try/ps_neutral.png');
  }
  #{{previewId}} {
  width: 100%;
  height: 100%;
  }
  .pane-container {
  overflow-x: hidden;
  overflow-y: scroll;
  height: 100%;
  padding: 0px;
  }
</style>


<div class="row">
  <div class="col-md-5 pane-container" id="admin-container">
    <nav class="navbar navbar-branded" role="navigation">
      <div class="container-fluid">
        <div class="collapse navbar-collapse text-center">
          <ul class="nav navbar-nav">
            <li>
              <a href="#" class="navbar-item" data-hull-action="exit">
                <i class="icon icon-exit"></i>
                &nbsp;
                Exit
              </a>
            </li>
          </ul>
          <p class="navbar-text">Customize</p>
          <ul class="nav navbar-nav navbar-right">
            <li>
              <a href="#" class="navbar-item" data-hull-action="saveTheShip">
                <i class="icon icon-valid"></i>
                &nbsp;
                Save
              </a>
            </li>
          </ul>
        </div>
      </div>
    </nav>

    <ul class="nav nav-tabs nav-justified" role="tablist">
      {{#each sections}}
        <li><a href="#" class="navbar-item" data-hull-action="displaySection" data-hull-section="{{@key}}">{{schema.title}}</a></li>
      {{/each}}
    </ul>

    <div data-editor style="padding: 20px;"></div>

  </div>
  <div class="col-md-7 pane-container ship-preview-container">
    <iframe src="{{previewUrl}}" frameborder="0" id="{{previewId}}"></iframe>
  </div>
</div>




